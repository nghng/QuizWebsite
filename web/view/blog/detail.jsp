        <section class="right__side">
            <div class="searchbox_wrapper">
                <form action="#">
                    <div class="searchbox__searchbar">
                        <input type="text" name="" id="" placeholder="Type something to search...">
                    </div>
                    <div class="searchbox_category">
                        <h5>Category</h5>
                        <div class="searchbox__option">
                            <div class="searchbox__option-item">
                                <input type="checkbox" name="" id=""> <span>Category 1</span>
                            </div>
                            <div class="searchbox__option-item">
                                <input type="checkbox" name="" id=""> <span>Category 2</span>
                            </div>
                            <div class="searchbox__option-item">
